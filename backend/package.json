{
  "name": "backend",
  "version": "1.0.0",
  "description": "Wedding Service Backend with Clean Architecture",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts",
    "db:setup": "npm run migrate:latest && npm run seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/knex": "^0.15.2",
    "@types/node": "^24.7.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "migrate": "^2.1.0",
    "module-alias": "^2.2.3",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  }
}
